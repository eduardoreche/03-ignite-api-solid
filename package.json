    "@prisma/extension-accelerate": "1.1.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.15.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.12.12",
    "@types/supertest": "6.0.2",
    "eslint": "8.36.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.14.0",
